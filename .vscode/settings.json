  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "Function name",
        "scope": "entity.name.function",
        "settings": {
          "foreground": "#9dff2c"
        }
      },
      {
        "name": "Keyword",
        "scope": "keyword",
        "settings": {
          "foreground": "#f71eae"
        }
      },
      {
        "name": "Comment",
        "scope": "comment",
        "settings": {
          "foreground": "#666666"
        }
      },
      {
        "name": "String",
        "scope": "string",
        "settings": {
          "foreground": "#ffc728"
        }
      },
      {
        "name": "Variable",
        "scope": "variable",
        "settings": {
          "foreground": "#F8F8F2"
        }
      },
      {
        "name": "Class name",
        "scope": "entity.name.class",
        "settings": {
          "foreground": "#9dff2c"
        }
      },
      {
        "name": "Parameter",
        "scope": "variable.parameter",
        "settings": {
          "foreground": "#ff661c",
          "fontStyle": "italic"
        }
      },
      {
        "name": "Storage type",
        "scope": "storage.type",
        "settings": {
          "foreground": "#409bff",
          "fontStyle": "italic"
        }
      },
      {
        "name": "Tag name",
        "scope": "entity.name.tag",
        "settings": {
          "foreground": "#f71eae"
        }
      },
      {
        "name": "Attribute name",
        "scope": "entity.other.attribute-name",
        "settings": {
          "foreground": "#9dff2c"
        }
      },
      {
        "name": "Number",
        "scope": "constant.numeric",
        "settings": {
          "foreground": "#AE81FF"
        }
      },
      {
        "name": "Built-in constant",
        "scope": "constant.language",
        "settings": {
          "foreground": "#AE81FF"
        }
      },
      {
        "name": "Invalid",
        "scope": "invalid",
        "settings": {
          "foreground": "#f71eae"
        }
      },
      {
        "name": "This/self",
        "scope": "variable.language",
        "settings": {
          "foreground": "#ff661c"
        }
      }
    ]
